, 10), new Date(2000, 10, 11, 10, 10)],\n      options: [{\n        value: \"1\",\n        label: \"管理员\"\n      }, {\n        value: \"2\",\n        label: \"学员\"\n      }],\n      imageUrl: \"\",\n      uploadFile: \"\",\n      key: \"\"\n    };\n  },\n\n  methods: {\n    add() {\n      this.ruleForm = {};\n      this.dialogVisible = true;\n    },\n\n    mediaDelete() {\n      mediaDelete(this.ruleForm).then(res => {\n        this.search();\n        this.$message({\n          message: \"修改成功！\",\n          type: \"success\"\n        });\n        this.dialogVisible2 = false;\n      });\n    },\n\n    submit() {\n      this.$refs[\"ruleForm\"].validate(valid => {\n        if (valid) {\n          mediaUpdate(this.ruleForm.id, { ...this.ruleForm\n          }).then(res => {\n            this.search();\n          });\n        } else {\n          console.log(\"error submit!!\");\n          return false;\n        }\n      });\n    },\n\n    handleClose() {\n      this.dialogVisible = false;\n    },\n\n    handleClose2() {\n      this.dialogVisible2 = false;\n    },\n\n    loadTable() {},\n\n    search() {\n      mediaList({\n        title: this.title\n      }).then(res => {\n        this.tableData = res.data.list;\n        this.pager.total = res.data.total;\n      });\n    },\n\n    edit(data) {\n      this.ruleForm = { ...data\n      };\n      this.dialogVisible = true;\n    },\n\n    deleteRow(data) {\n      this.ruleForm = data;\n      this.dialogVisible2 = true;\n    },\n\n    handleAvatarSuccess(res, file) {\n      this.imageUrl = URL.createObjectURL(file.raw);\n    },\n\n    beforeAvatarUpload(file) {\n      this.uploadFile = file;\n    },\n\n    // 上传文件\n    getTmp_secret_keys(event, file, fileList) {\n      uploadInfo({\n        file_type: \"IMAGE\",\n        filename: file.raw.name\n      }).then(res => {\n        this.url = res.data.url;\n        this.key = res.data.key;\n        let that = this;\n        var data = new FormData();\n        data.append(\"file\", file); //获取文件\n\n        var config = {\n          onUploadProgress: function (progressEvent) {\n            //上传进度\n            var percentCompleted = Math.round(progressEvent.loaded * 100 / progressEvent.total);\n          }\n        };\n        axios.put(this.url, data, config).then(function (res) {\n          that.imageUrl = URL.createObjectURL(file.raw);\n          that.ruleForm.img = that.key;\n        }).catch(function (err) {});\n      });\n    }\n\n  },\n\n  mounted() {\n    this.search();\n  }\n\n};","map":{"version":3,"mappings":"AAoGA;AACA;AACA;AACA;EACAA;IACAC;EADA,CADA;;EAIAC;IACA;MACAC,qBADA;MAEAC,OAFA;MAGAC,oBAHA;MAIAC,qBAJA;MAKAC;QACAC;UAAAC;UAAAC;UAAAC;QAAA,EADA;QAEAC;UAAAH;UAAAC;UAAAC;QAAA,EAFA;QAGAE;UAAAJ;UAAAC;UAAAC;QAAA,EAHA;QAIAG;UAAAL;UAAAC;UAAAC;QAAA,EAJA;QAKAP;UAAAK;UAAAC;UAAAC;QAAA,EALA;QAMAI;UAAAN;UAAAC;UAAAC;QAAA;MANA,CALA;MAaAK;QACAD;MADA,CAbA;MAgBAP,SAhBA;MAiBAS,aAjBA;MAiBA;MACAC,aACA;QAAAC;QAAAC;MAAA,CADA,EAEA;QAAAD;QAAAC;MAAA,CAFA,EAGA;QAAAD;QAAAC;MAAA,CAHA,EAIA;QAAAD;QAAAC;MAAA,CAJA,EAKA;QAAAD;QAAAC;MAAA,CALA,EAMA;QAAAD;QAAAC;MAAA,CANA,EAOA;QAAAD;QAAAP;QAAAQ;MAAA,CAPA,EAQA;QAAAD;QAAAP;QAAAQ;MAAA,CARA,CAlBA;MA4BAC;QACAC,YADA;QACA;QACAC,cAFA;QAEA;QACAC,SAHA,CAGA;;MAHA,CA5BA;MAiCAC,wEAjCA;MAkCAC,UACA;QACAC,UADA;QAEAR;MAFA,CADA,EAKA;QACAQ,UADA;QAEAR;MAFA,CALA,CAlCA;MA4CAS,YA5CA;MA6CAC,cA7CA;MA8CAC;IA9CA;EAgDA,CArDA;;EAsDAC;IACAC;MACA;MACA;IACA,CAJA;;IAKAC;MACAA;QACA;QACA;UACAvB,gBADA;UAEAE;QAFA;QAIA;MACA,CAPA;IAQA,CAdA;;IAeAsB;MACA;QACA;UACAC;UAAA;YACA;UACA,CAFA;QAGA,CAJA,MAIA;UACAC;UACA;QACA;MACA,CATA;IAUA,CA1BA;;IA2BAC;MACA;IACA,CA7BA;;IA8BAC;MACA;IACA,CAhCA;;IAiCAC,cAjCA;;IAkCAC;MACAC;QACAjC;MADA,GAEAkC,IAFA,CAEAC;QACA;QACA;MACA,CALA;IAMA,CAzCA;;IA0CAC;MACA;MAAA;MACA;IACA,CA7CA;;IA8CAC;MACA;MACA;IACA,CAjDA;;IAkDAC;MACA;IACA,CApDA;;IAqDAC;MACA;IACA,CAvDA;;IAwDA;IACAC;MACAC;QACAC,kBADA;QAEAC;MAFA,GAGAT,IAHA,CAGAC;QACA;QACA;QACA;QACA;QACAzC,0BALA,CAKA;;QACA;UACAkD;YACA;YACA,kCACAC,gDADA;UAGA;QANA;QAQAC,MACAC,GADA,CACA,QADA,EACArD,IADA,