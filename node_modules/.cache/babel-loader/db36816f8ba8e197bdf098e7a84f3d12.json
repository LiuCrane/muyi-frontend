dTable() {},\n\n    edit(data) {\n      this.ruleForm = { ...data\n      };\n      this.dialogVisible = true;\n    },\n\n    search() {\n      storesList({\n        manager_name: this.manager_name,\n        name: this.name\n      }).then(res => {\n        this.tableData = res.data.list;\n        this.pager.total = res.data.total;\n      });\n    },\n\n    cancel() {\n      cancel(this.ruleForm.id).then(res => {\n        this.search();\n        this.$message({\n          message: \"注销成功！\",\n          type: \"success\"\n        });\n        this.dialogVisible = false;\n      });\n    },\n\n    submit() {\n      this.$refs[\"ruleForm\"].validate(valid => {\n        if (valid) {\n          storesUpdate(this.ruleForm.id, { ...this.ruleForm\n          }).then(res => {\n            this.search();\n            this.$message({\n              message: \"修改成功！\",\n              type: \"success\"\n            });\n            this.dialogVisible = false;\n          });\n        } else {\n          console.log(\"error submit!!\");\n          return false;\n        }\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAiEA;AACA;AACA;EACAA;IACAC;EADA,CADA;;EAIAC;IACA;MACAC,YADA;MAEAC;QACAC,eACA;UAAAC;UAAAC;UAAAC;QAAA,CADA,CADA;QAIAC;UAAAH;UAAAC;UAAAC;QAAA,EAJA;QAKAE;UAAAJ;UAAAC;UAAAC;QAAA,EALA;QAMAG,gBACA;UAAAL;UAAAC;UAAAC;QAAA,CADA;MANA,CAFA;MAYAI,oBAZA;MAaAP,gBAbA;MAcAK,QAdA;MAeAG,aAfA;MAeA;MACAC,aACA;QAAAC;QAAAC;MAAA,CADA,EAEA;QAAAD;QAAAC;MAAA,CAFA,EAGA;QAAAD;QAAAC;MAAA,CAHA,EAIA;QAAAD;QAAAC;MAAA,CAJA,EAKA;QAAAD;QAAAC;MAAA,CALA,EAMA;QAAAD;QAAAC;MAAA,CANA,EAOA;QAAAD;QAAAC;MAAA,CAPA,EAQA;QAAAD;QAAAE;QAAAD;MAAA,CARA,CAhBA;MA0BAE;QACAC,YADA;QACA;QACAC,cAFA;QAEA;QACAC,QAHA,CAGA;;MAHA,CA1BA;MA+BAC,wEA/BA;MAgCAC,UACA;QACAC,UADA;QAEAT;MAFA,CADA,EAKA;QACAS,UADA;QAEAT;MAFA,CALA,CAhCA;MA0CAS;IA1CA;EA4CA,CAjDA;;EAkDAC;IACA;EACA,CApDA;;EAqDAC;IACAC;MACA;IACA,CAHA;;IAIAC,cAJA;;IAKAC;MACA;MAAA;MACA;IACA,CARA;;IASAC;MACAC;QAAA1B;QAAAK;MAAA,QACAsB;QACA;QACA;MACA,CAJA;IAMA,CAhBA;;IAiBA